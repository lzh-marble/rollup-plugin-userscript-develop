{
  "name": "@lzh-marble/rollup-plugin-userscript-develop",
  "version": "0.0.2",
  "description": "a template for building library on node, which use rollup and supports typescript",
  "author": {
    "name": "lzh-marble",
    "email": "lzh.marble@gmail.com"
  },
  "license": "ISC",
  "scripts": {
    "build-file": "rollup -c",
    "build-declaration": "tsc --declaration --emitDeclarationOnly",
    "build": "npm-run-all --parallel build-file build-declaration",
    "prod": "NODE_ENV=prod npm run build",
    "start": "nodemon --exec npm run build --watch src -e js,ts"
  },
  "peerDependencies": {
    "@babel/generator": ">=7.0.0",
    "@babel/parser": ">=7.0.0",
    "@babel/template": ">=7.0.0",
    "@babel/traverse": ">=7.0.0",
    "@babel/types": ">=7.0.0"
  },
  "dependencies": {
    "@babel/generator": "^7.21.1",
    "@babel/parser": "^7.21.2",
    "@babel/template": "^7.20.7",
    "@babel/traverse": "^7.21.2",
    "@babel/types": "^7.21.2"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.1.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@rollup/plugin-terser": "^0.4.0",
    "@types/node": "^18.14.2",
    "core-js": "^3.29.0",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.79.1",
    "typescript": "^4.9.5"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=14.0.0"
  }
}
